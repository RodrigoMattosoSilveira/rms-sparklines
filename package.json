{
  "name": "rms-sparklines",
  "version": "0.3.0",
  "scripts": {
    "ng": "ng",
    "start:aot": "ng serve --port 4201 --aot=true",
    "start:jit": "ng serve --port 4201 --aot=false",
    "build": "ng build",
    "build:lib": "ng build library",
    "build:serve": "ng build library && yarn start:aot",
    "build:lsg": "ng build living-style-guide",
    "build:all": "yarn build:lib && yarn build:lsg",
    "build:reset": "rm -rf node_modules && yarn install && yarn build:all",
    "serve": "ng serve living-style-guide --port 4201",
    "test": "ng test",
    "test:lib": "ng test library",
    "test:lsg": "ng test living-style-guide",
    "test:all": "ng test living-style-guide --watch=false && ng e2e",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "mathjs": "^5.10.3",
    "ngx-markdown": "^7.1.5",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.7",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
