{
  "name": "rms-sparklines",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RodrigoMattosoSilveira/rms-sparklines"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "ng build rmstek-sparklines --prod",
    "lib:build:link": "ng build rmstek-sparklines --prod && npm link ./dist/rmstek-sparklines",
    "lib:dev": "ng build rmstek-sparklines && rm -rf node_modules/@rmstek/sparklines && cp -r dist/rmstek-sparklines node_modules/@rmstek/sparklines",
    "lib:build:watch": "ng build rmstek-sparklines  --prod --watch",
    "lib:chglog": "cp ./projects/rmstek-sparklines/CHANGELOG.md ./dist/rmstek-sparklines",
    "lib:license": "cp ./projects/rmstek-sparklines/LICENSE ./dist/library",
    "lib:readme": "cp ./projects/rmstek-sparklines/README.md ./dist/rmstek-sparklines",
    "lib:clr": "run-s lib:chglog lib:license lib:readme",
    "lib:pack": "run-s lib_setup cd dist/rmstek-sparklines npm pack",
    "lib:ready": "run-s lib:build lib:clr lib:pack",
    "lib:test": "ng test rmstek-sparklines",
		"lib:test:nowatch": "ng test rmstek-sparklines --nowatch",
    "lib:link": "npm link ./dist/rmstek-sparklines",
    "reset:node_modules": "rm -rf node_modules/",
    "reset:package_lock": "rm -f package-lock.json",
    "reset:all": "npm run reset:node_modules && npm run reset:package_lock && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@types/mathjs": "6.0.4",
    "core-js": "^3.5.0",
    "mathjs": "^6.6.1",
    "ngx-markdown": "^9.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.9.5",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-md5": "^1.2.4",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
