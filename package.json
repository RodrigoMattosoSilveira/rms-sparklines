{
  "name": "rms-sparklines",
  "version": "0.3.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:add": "yarn add ./dist/rmstek-sparklines/ -O",
    "lib:build": "ng build rmstek-sparklines",
    "lib:dev": "ng build rmstek-sparklines && rm -rf node_modules/@rmstek/sparklines && cp -r dist/rmstek-sparklines node_modules/@rmstek/sparklines",
    "lib:build:watch": "ng build rmstek-sparklines --watch",
    "lib:chglog": "cp ./projects/rmstek-sparklines/CHANGELOG.md ./dist/rmstek-sparklines",
    "lib:license": "cp ./projects/rmstek-sparklines/LICENSE ./dist/library",
    "lib:readme": "cp ./projects/rmstek-sparklines/README.md ./dist/rmstek-sparklines",
    "lib:clr": "yarn lib:chglog && yarn lib:license && yarn lib:readme",
    "lib:pack": "yarn lib_setup && cd dist/rmstek-sparklines && npm pack",
    "lib:ready": "yarn lib:build && yarn lib:clr && yarn lib:pack",
    "lib:reset": "yarn npm:reset && yarn lib:build && rm -rf node_modules/ && yarn install && yarn lib:add",
    "lib:test": "ng test rmstek-sparklines",
    "npm:reset": "rm -f yarn.lock && rm -rf node_modules/ && npm install && rm -f package-lock.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@types/mathjs": "^5.0.1",
    "core-js": "^2.5.4",
    "mathjs": "^6.0.2",
    "ngx-markdown": "^7.1.5",
    "rxjs": "~6.5.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "ts-md5": "^1.2.4",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.10.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  },
  "optionalDependencies": {
    "@rmstek/sparklines": "./dist/rmstek-sparklines/"
  }
}
